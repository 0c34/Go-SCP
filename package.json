{
  "name": "go-webapp-scp",
  "version": "0.0.0",
  "description": "Go Web Applications Secure Coding Practices",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "gitbook-cli": "^2.3.0"
  },
  "config": {
    "lang": "en"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "gitbook serve src/$npm_package_config_lang _book/$npm_package_config_lang",
    "build-pdf": "gitbook pdf src/$npm_package_config_lang ./build/$npm_package_name-$npm_package_config_lang.pdf",
    "build-epub": "gitbook epub src/$npm_package_config_lang ./build/$npm_package_name-$npm_package_config_lang.epub",
    "build-mobi": "gitbook mobi src/$npm_package_config_lang ./build/$npm_package_name-$npm_package_config_lang.mobi",
    "build": "npm run build-pdf; npm run build-epub; npm run build-mobi"
  },
  "keywords": [
    "Go",
    "WebApp",
    "Security",
    "Checkmarx"
  ],
  "author": "Checkmarx Security Research Team",
  "license": "GPL-3.0"
}
